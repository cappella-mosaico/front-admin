export const Eventos = ({token}) => {

  const [eventos, setEventos] = useState([]);

  useEffect(() => {
    if (token) {
      fetch(`${ROOT_URL}/eventos`)
        .then(response => response.json())
        .then(d => {
          setEventos(d);
        })
        .catch(error => console.error(error));
    }
  }, [token, setPastorais, pastorais.length]);

  return ();
}
